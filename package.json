{
  "name": "trocapp",
  "version": "1.0.0",
  "description": "Trocapp",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "stop": "pm2 stop all",
    "start": "pm2 start ecosystem.config.js -i max --attach --env production",
    "start:dev": "mkdirp ./tmp/dev-server && node index.js",
    "test": "jest --forceExit --coverage --no-cache --detectOpenHandles",
    "test:dev": "jest --watch --coverage --detectOpenHandles",
    "coverage": "yarn test && google-chrome ./coverage/lcov-report/index.html",
    "eslint:ci": "eslint src -c .eslintrc --format html -o coverage/eslint.html",
    "eslint:dev": "npm-run-all lint:*",
    "acceptance": "cd acc && cucumber chrome=true test=true",
    "lint:js": "eslint --fix --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint:css": "stylelint --fix \"src/**/*.{css,scss,sass}\"",
    "clean": "npm-run-all clean:*",
    "clean:build": "rimraf build",
    "clean:tmp": "rimraf tmp",
    "build": "NODE_ENV=production PATH_CONTEXT=/ npm-run-all clean:build build:*",
    "build:server": "webpack --mode production --config webpack.config.server.js",
    "build:client": "webpack --mode production --config webpack.config.client.js",
    "release": "yarn standard-version"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "git add --force"
    ]
  },
